@model CareHomeMock.Models.CareHome
@{
    ViewBag.Title = "CareHomeInfo_AccessMap";
}
@section scripts{
    <style type="text/css">
        #map-canvas {
            width: 400px;
            height: 400px;
            margin: 0;
            padding: 0;
        }
    </style>
    <script type="text/javascript"
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCGq4kB5UwEsJphzWLAaNoASJiEOpod1KM"></script>
    <script type="text/javascript">
        function initialize() {
            var mapOptions = {
                center: { lat: @Model.Latitude, lng: @Model.Longitude },
                zoom: 8
            };
            var map = new google.maps.Map(document.getElementById('map-canvas'),
                mapOptions);
        }
        google.maps.event.addDomListener(window, 'load', initialize);
    </script>
}

<h2>CareHomeInfo_AccessMap</h2>
<div id="map-canvas"></div>
